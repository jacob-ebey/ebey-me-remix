{
  "private": true,
  "name": "ebey-me",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup && run-s build:tailwind build:remix",
    "build": "run-s build:tailwind build:remix",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:tailwind": "cross-env NODE_ENV=production tailwindcss -o app/styles/tailwind.css",
    "start": "dotenv cross-env NODE_ENV=production remix-serve api/build",
    "dev": "run-s dev:tailwind dev:remix",
    "dev:remix": "dotenv cross-env NODE_ENV=development remix run",
    "dev:tailwind": "cross-env NODE_ENV=development tailwindcss -o app/styles/tailwind.css"
  },
  "dependencies": {
    "@remix-run/node": "0.18.0-pre.3",
    "@remix-run/react": "0.18.0-pre.3",
    "@remix-run/serve": "0.18.0-pre.3",
    "@remix-run/vercel": "0.18.0-pre.3",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.11.0",
    "fecha": "^4.2.1",
    "form-data": "^4.0.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.2",
    "remix": "0.18.0-pre.3"
  },
  "devDependencies": {
    "@remix-run/dev": "0.18.0-pre.3",
    "@tailwindcss/typography": "^0.4.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
